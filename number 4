#include <stdio.h>
#include <string.h>
int main() {
    char c[1000];
    scanf("%s", c);
    int sum = 0, i;
    for (i = 0; i < strlen(c); i++) {
        char ch = c[i];
        int d = ch - '0';
        if (d >= 0 && d <= 9) {
            sum = sum + d;
        }
    }   printf("Sum of digits: %d\n", sum);
    return 0;}
